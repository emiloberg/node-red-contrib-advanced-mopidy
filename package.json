{
  "name": "node-red-contrib-advanced-mopidy",
  "version": "1.2.0",
  "description": "A Node-RED node to communicate with Mopidy",
  "scripts": {
    "postinstall": "./node_modules/.bin/babel --stage 1 --copy-files --out-dir ./mopidy ./src",
    "test": "./node_modules/.bin/mocha --recursive --grep 'MopidyConnected' --invert",
    "test-all": "./node_modules/.bin/mocha --recursive",
    "lint": "./node_modules/.bin/eslint ./src",
    "watch": "./node_modules/.bin/babel --stage 1 --copy-files --watch --out-dir ./mopidy ./src",
    "clean-build": "./node_modules/.bin/rimraf mopidy; ./node_modules/.bin/babel --stage 1 --copy-files --out-dir ./mopidy ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emiloberg/node-red-contrib-advanced-mopidy.git"
  },
  "keywords": [
    "node-red",
    "node-red-contrib",
    "contrib",
    "ping"
  ],
  "node-red": {
    "nodes": {
      "file-function": "mopidy/mopidy.js"
    }
  },
  "author": {
    "name": "Emil Oberg"
  },
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/emiloberg/node-red-contrib-advanced-mopidy/issues"
  },
  "homepage": "https://github.com/emiloberg/node-red-contrib-advanced-mopidy",
  "dependencies": {
    "babel": "^5.8.23",
    "mopidy": "^0.5.0",
    "promise": "^7.0.4",
    "rimraf": "^2.4.3"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "chai-things": "^0.2.0",
    "eslint": "^1.5.1",
    "mocha": "^2.3.3"
  }
}
